package application;

import java.util.ArrayList;
import java.util.List;

public class Program {

	public static void main(String[] args) {
		
		List<Integer> list = new ArrayList<>();
		
		list.add(18);
		list.add(35);
		list.add(20);
		list.add(12);
		list.add(2, 01);
		
		System.out.println(list.size);
		
		for (Integer x : list) {
			System.out.println(x);
		}
	}

}
